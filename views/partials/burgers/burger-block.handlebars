<li>
	{{name}}<br>
{{#if eaten}}
	<button class="change-eat" data-id="{{id}}" data-eat="{{eaten}}">
	Eat me!
	</button>
{{/if}}


{{#unless eaten}}
	<button class="rate" data-id="{{id}}" data-rate="5">❤️</button>
	<button class="rate" data-id="{{id}}" data-rate="4">⭐</button>
	<button class="rate" data-id="{{id}}" data-rate="3">👍</button>
	<button class="rate" data-id="{{id}}" data-rate="2">👎</button>
	<button class="rate" data-id="{{id}}" data-rate="1">🤮</button>
{{/unless}}


</li>
